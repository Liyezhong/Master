			value1_send_data_message="1"
                        value1="200"
                        value2_send_at_exceed="0"
                        value2_send_at_fall_below="0"
                        value2_send_warn_message="0"
                        value2_send_data_message="0"
                        value2="0"
                        hysteresis="0"
