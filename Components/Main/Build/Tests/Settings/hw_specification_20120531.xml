                <functionmodule type="analog_out" key="transmit_control" name="transmit control" interface="2">
                    <configuration
                        enabled="1"
                        inactiv_shutdown="1"
                        inactiv_emcy="1"
                        resolution="16"
                        outval_incactiv="65535"
                        livetime_limit="65535"
